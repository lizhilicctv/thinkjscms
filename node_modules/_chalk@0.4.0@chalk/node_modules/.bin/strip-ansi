#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_strip-ansi@0.1.1@strip-ansi/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_strip-ansi@0.1.1@strip-ansi/cli.js" "$@"
  ret=$?
fi
exit $ret
